{
  "extends": "@parcel/config-default",
  "optimizers": {
    "*.js": [
      "...",
      "@parcel/optimizer-esbuild"
    ],
    "*.css": [
      "@parcel/optimizer-cssnano"
    ]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  },
  "reporters": [
    "...",
    "@parcel/reporter-bundle-analyzer"
  ]
}
